# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# Map static resources from the /public folder to the /assets URL path
GET         /assets/*file        controllers.Assets.versioned(path="/public", file: Asset)

GET         /cats                controllers.CategoryController.list
GET         /cats/:id            controllers.CategoryController.get(id: Int)
POST        /cats                controllers.CategoryController.create
PUT         /cats/:id            controllers.CategoryController.update(id: Int)

GET         /account             controllers.AccountController.getAccounts
POST        /account             controllers.AccountController.createAccount

POST        /login               controllers.AccountController.login

# POST /receiveVideoStream controllers.StreamController.get